{
  "log": {
    "level": "info",
    "output": "/var/log/sing-box.log",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "direct-dns",
        "type": "udp",
        "server": "1.1.1.1"
      }
    ],
    "strategy": "ipv4_only"
  },
  "inbounds": [
    {
      "type": "tproxy",
      "tag": "tproxy-in",
      "listen": "0.0.0.0",
      "listen_port": 7893,
      "sniff": true,
      "sniff_override_destination": false
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "selector",
      "tag": "default-exit",
      "outbounds": [
        "direct"
      ],
      "default": "direct",
      "interrupt_exist_connections": false
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "block",
      "tag": "adblock"
    }
  ],
  "endpoints": [],
  "route": {
    "rules": [
      {
        "action": "sniff",
        "sniffer": [
          "tls",
          "http",
          "quic",
          "bittorrent",
          "stun",
          "dtls"
        ],
        "timeout": "300ms"
      },
      {
        "action": "hijack-dns",
        "protocol": "dns"
      },
      {
        "ip_cidr": [
          "10.1.1.0/24"
        ],
        "outbound": "direct"
      }
    ],
    "rule_set": [],
    "final": "default-exit"
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "secret": ""
    },
    "v2ray_api": {
      "listen": "127.0.0.1:10085",
      "stats": {
        "enabled": true,
        "outbounds": [
          "direct",
          "block",
          "adblock"
        ]
      }
    },
    "cache_file": {
      "enabled": true,
      "path": "/etc/sing-box/cache.db"
    }
  }
}